import java.util.*;

public class Compte {
	private int solde;
	private String NomCompte;
	

	  public Compte()
	  {
	      this.NomCompte = "Compte";
	      this.solde = -1;
	  }

	public void virement()
	{
		

	}

	public int getSolde()
	{
	    return  this.solde;
	}

	public void affSolde()
	{
	  System.out.println(this.NomCompte+ " : "  + this.getSolde());
	}

}
